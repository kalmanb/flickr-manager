{
  "name": "flickr-manager",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --harmony test",
    "watch": "nodemon --exec \"npm run test\" --watch src --watch test",
